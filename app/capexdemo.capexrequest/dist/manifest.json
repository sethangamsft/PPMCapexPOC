{
	"_version": "1.60.0",
	"sap.app": {
		"id": "capexdemo.capexrequest",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			]
		},
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "@sap/generator-fiori:lrop",
			"version": "1.20.0",
			"toolsId": "0172a5a9-97d9-42af-a59c-0621bea5cf00"
		},
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/capexservice/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"capexdemocapexrequest-display": {
					"semanticObject": "capexdemocapexrequest",
					"action": "display",
					"title": "{{capexdemocapexrequest-display.flpTitle}}",
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					}
				},
				"capexrequest-open": {
					"semanticObject": "capexrequest",
					"action": "open",
					"title": "{{capexrequest-open.flpTitle}}",
					"subTitle": "{{capexrequest-open.flpSubtitle}}",
					"icon": "sap-icon://open-folder",
					"indicatorDataSource": {
						"dataSource": "mainService",
						"path": "CapexRequest",
						"refresh": 1800
					},
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					}
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": true,
		"dependencies": {
			"minUI5Version": "1.120.13",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "capexdemo.capexrequest.i18n.i18n",
					"supportedLocales": [
						""
					]
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties",
				"settings": {
					"supportedLocales": [
						""
					]
				}
			}
		},
		"resources": {
			"css": []
		},
		"routing": {
			"config": {},
			"routes": [
				{
					"pattern": ":?query:",
					"name": "CapexRequestList",
					"target": "CapexRequestList"
				},
				{
					"pattern": "CapexRequest({key}):?query:",
					"name": "CapexRequestObjectPage",
					"target": "CapexRequestObjectPage"
				},
				{
					"pattern": "CapexRequest({key})/approvals({key2}):?query:",
					"name": "CapexApprovalObjectPage",
					"target": "CapexApprovalObjectPage"
				}
			],
			"targets": {
				"CapexRequestList": {
					"type": "Component",
					"id": "CapexRequestList",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"contextPath": "/CapexRequest",
							"initialLoad": "Enabled",
							"variantManagement": "Page",
							"navigation": {
								"CapexRequest": {
									"detail": {
										"route": "CapexRequestObjectPage"
									}
								}
							},
							"controlConfiguration": {
								"@com.sap.vocabularies.UI.v1.LineItem": {
									"tableSettings": {
										"type": "ResponsiveTable"
									}
								}
							}
						}
					}
				},
				"CapexRequestObjectPage": {
					"type": "Component",
					"id": "CapexRequestObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"editableHeaderContent": false,
							"contextPath": "/CapexRequest",
							"navigation": {
								"approvals": {
									"detail": {
										"route": "CapexApprovalObjectPage"
									}
								}
							}
						}
					}
				},
				"CapexApprovalObjectPage": {
					"type": "Component",
					"id": "CapexApprovalObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"editableHeaderContent": false,
							"contextPath": "/CapexRequest/approvals"
						}
					}
				}
			}
		},
		"flexBundle": false
	},
	"sap.fiori": {
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.cloud": {
		"public": true,
		"service": "capexdemo.service"
	}
}