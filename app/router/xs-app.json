{
  "authenticationMethod": "route",
  "welcomeFile": "/index.html",
  "routes": [
    {
      "source": "^/(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "cacheControl": "public, max-age=3600",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v4/(.*)$",
      "target": "/odata/v4/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "csrfProtection": true
    }
  ]
}
